generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id       String  @id @default(cuid())
  email    String  @unique
  isPro    Boolean @default(false)
  fpDone   Boolean @default(true)
  priority Boolean @default(false)
  usage    Int     @default(0)
}
